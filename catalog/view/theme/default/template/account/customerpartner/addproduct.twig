    $('select[name="wkcustomfield"]').on('change',function(){
      value = $(this).val();
      if(value == ''){
        return;
      }

      if($('body').find('[field-id = wkoption-'+value+']').length){
        return;
      }
    fieldType = $('option:selected', this).attr('data-type');
    fieldName = $('option:selected', this).attr('data-name');
