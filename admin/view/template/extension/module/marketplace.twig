  html = '<i class="fa fa-minus-circle" onclick="$(\'a[href=\\\'#tab-module'+tabId+'\\\']\').parent().remove(); $(\'#tab-module'+tabId+'\').remove(); $(\'#module a:first\').tab(\'show\');"></i> ';
