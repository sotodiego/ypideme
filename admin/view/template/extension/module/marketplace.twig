                            <input type="hidden" name="marketplace_boxcommission[{{ key }}]" value="{{ key}}"/>
                            {% if key == 'fixed' %}
                              {{ entry_fixed }}
                            {% elseif key == 'category' %}
                              {{ entry_category }}
                            {% elseif key == 'category_child' %}
                              {{ entry_category_child}}
                            {% endif %}
                            </a>
                          </li>
                        {% endfor %}
                      {% endif %}
                    {% endif %}
                  </ul>
                </div>
              </div>
            </div>


